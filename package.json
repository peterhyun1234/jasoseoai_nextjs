{
  "name": "jasoseoai_nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4001",
    "build:beta": "env-cmd -f .env.development next build",
    "build:release": "env-cmd -f .env.production next build",
    "start": "next start -p 4001",
    "pm2:beta": "pm2 start ecosystem.config.json --only jasoseoai-nextjs-dev",
    "pm2:release": "pm2 start ecosystem.config.json --only jasoseoai-nextjs-prod",
    "submodule:init": "git submodule update --init",
    "submodule": "git submodule foreach npm install",
    "submodule:update": "git submodule update --remote --merge",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.8",
    "@mui/styled-engine-sc": "^5.11.0",
    "@next/font": "13.1.6",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "axios": "^1.3.3",
    "env-cmd": "^10.1.0",
    "eslint-config-next": "13.1.6",
    "husky": "^8.0.3",
    "next": "13.1.6",
    "next-auth": "^4.19.2",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^8.0.5",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "remark-gfm": "^3.0.1",
    "smoothscroll-polyfill": "^0.4.4",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/react-swipeable-views": "^0.13.1",
    "@types/react-swipeable-views-utils": "^0.13.4",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.1.1",
    "prettier": "2.8.4",
    "typescript": "^4.9.5"
  }
}
